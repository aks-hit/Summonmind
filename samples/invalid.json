{
  "schema_def": {
    "version": 1,
    "fields": {
      "firstName": "string",
      "age": "number"
    }
  },
  "rules": [
    {
      "id": "r1",
      "level": "field",
      "field": "age",
      "condition": "value >= 18",
      "action": "validate"
    },
    {
      "id": "r2",
      "level": "field",
      "field": "firstName",
      "condition": "len(value) >= 2",
      "action": "validate"
    }
  ],
  "data": {
    "firstName": "A",
    "age": 20
  }
}
